{
  "manifest_version": 2,
  "name": "Количество выгрузок",
  "version": "2.0",
  "description": "Отчет по работе",
  "applications": {
    "gecko": {
      "id": "Report_works@advance-docs.ru",
      "strict_min_version": "49.0"
    }
  },
  "sidebar_action": {
  "default_title": "Works",
  "default_panel": "sidebar/sidebar.html",
  "default_icon": "sidebar/sidebar_icon.png",
  "browser_style": true
},
"options_ui": {
  "page": "options/options.html",
  "browser_style": true
},

  "permissions": [
      "storage",
      "activeTab",
      "tabs",
      "notifications",
      "<all_urls>",
      "*://10.250.4.13/*",
      "*://*.advance-docs.ru/*",
      "webRequest",
      "webRequestBlocking",
      "webNavigation",
      "downloads",
      "cookies"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
